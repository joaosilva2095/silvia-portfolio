<footer class="row no-gutters justify-content-end align-items-center">
    <p class="mb-0 mr-5 p-0">Contactos</p>
    {% for item in socialMedia %}
        <a class="h-100 mx-3 d-flex align-items-center social-icon"
           href="{{ item.link }}" target="_blank"
           rel="noopener noreferrer"
           title="{{ item.title }}" itemprop="url">
            <i class="{{ item.icon }}" aria-hidden="true"></i>
        </a>
    {% endfor %}
</footer>
