<!doctype html>
<html lang="en" class="h-100">

<head>
    {% include 'abstract/head.nj' %}
</head>

<body class="h-100" data-spy="scroll" data-target="#navbar" data-offset="0">
<main class="h-100">
    {% include 'components/navbar.nj' %}
    {% include 'components/header.nj' %}

    <!-- Gallery -->
    <section id="gallery" class="row no-gutters mt-5 ml-lg-2 mr-lg-5">
        <div class="col-12 offset-lg-1 col-lg-8">
            <div class="row no-gutters">
                {% import "components/gallery-item.nj" as gallery %}
                {% for item in galleryItems %}
                    <div class="col-12 col-lg-4">
                        {{ gallery.item("https://pbs.twimg.com/profile_images/949374088249671680/MuxDEZpD_400x400.jpg") }}
                    </div>
                {% endfor %}
            </div>
        </div>
    </section>
</main>
</body>

{% include 'abstract/scripts.nj' %}
</html>
